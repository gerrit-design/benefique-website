{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/terms",
      "destination": "/terms.html",
      "permanent": false
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
